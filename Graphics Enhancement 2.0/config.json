      },
	  {
         "Name": "Performance",
         "DisplayName": "Visual downgrades for performance",
         "Elements": [
      {
               "Name": "Perf_AresDent",
               "DisplayName": "Disable Ares/Chaos sand/snow footsteps:",
               "Description": [
                  "Around a 10% increase in performance."
               ],
               "Type": "Perf_AresDent",
               "MinValue": 0,
               "MaxValue": 99,
               "DefaultValue": "0"

      },
	  {
               "Name": "MipMapBias",
               "DisplayName": "(Read desc.) Use lower quality mip maps (textures):",
               "Description": [
                  "Sets lower quality mip maps to be used when closer, leading to a lower video memory (VRAM) usage. This may or may not bring a performance increase depending on your system. This will not work unless you set the anti-aliasing setting to TAA in the game's Graphics Settings."
               ],
               "Type": "MipMapBias",
               "MinValue": 0,
               "MaxValue": 99,
               "DefaultValue": "0"

      },
      {
               "Name": "Perf_TerrainBlend",
               "DisplayName": "Disable Terrain Blend:",
               "Description": [
                  "Overwrites 'Terrain Texture Blending extension'. Around a 10% increase in performance."
               ],
               "Type": "Perf_TerrainBlend",
               "MinValue": 0,
               "MaxValue": 99,
               "DefaultValue": "0"
      },
      {
               "Name": "NoBloom",
               "DisplayName": "Disable Bloom:",
               "Description": [
                  ""
               ],
               "Type": "NoBloom",
               "MinValue": 0,
               "MaxValue": 99,
