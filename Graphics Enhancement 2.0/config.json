               ],
               "Type": "SunPos",
               "MinValue": 0,
               "MaxValue": 99,
               "DefaultValue": "0"
            },
        {
               "Name": "GEGrassDitherConfig",
               "DisplayName": "Decrease Grass fade out distance:",
               "Description": [
                  "Decreases the distance from the camera in which grass starts disappering. FPS will fluke when panning the camera across grass."
               ],
               "Type": "GEGrassDitherConfig",
               "MinValue": 0,
               "MaxValue": 99,
